                                                                    total = parseInt(qty) * parseFloat(price,10);
                                                                    if(qty != null && qty != ''){total = parseInt(qty) * parseFloat(price,10);                                                                                                                                              
                                                                        prodTotal.find('.prodTotalNum').text(total.toFixed(2));
                                                                    }
                                                                    allTotal = 0; 
                                                                    cartTable.find('.prodTotalNum').each(function(){
                                                                        allTotal += parseFloat($(this).text());
                                                                    });
                                                                    cartTable.find('.rowFooter').find('.subtotalNum').text(allTotal.toFixed(2));                                                                    
                                                                })                                                          
                                                            )
                                                        )
