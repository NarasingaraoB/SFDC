                            var allTotal = 0; 
                            
    var vn=document.getElementById('hname').value;
  var vi=document.getElementById('hid').value;
    
         alert(parseFloat(itemRow1.find('.prodQty input').val())+1);
                                             
                            //if the prod is already in the cart then call the increment and total operation by invoking click on the input
