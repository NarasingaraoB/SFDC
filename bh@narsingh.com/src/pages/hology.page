                                var item = prodData.find('.name').text();
                                console.log('item:'+item);
                                section2.find('.ui-grid-a.products').find(".prodContainer:contains('"+item+"')").click();
                                $(this).parent().append($('<span class="plus">+1</span>').show().stop().animate({ opacity: 0, top: '-15px'}, 'slow', function(){$(this).remove();}));
                            });
                            
                            $('#prodPop').find('.block').empty();
                            $('#prodPop').find('.block').append(prodData.show());
                            
                            event.stopPropagation();
                        });
                                            
