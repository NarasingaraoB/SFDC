                                                                            allTotal += parseFloat($(this).text());
                                                                        });
                                                                        cartTable1.find('.rowFooter').find('.subtotalNum').text(allTotal.toFixed(2));                                                                    
                                                                    })                                                                  
                                                                )
                                                            .append('<span>'+item+'</span>')                                                    
                                                        )
                                                .append($('<td class="prodQty"></td>')
                                                            .append($('<input type="number" min="0" />').val(1)
                                                                .unbind('keyup click').bind("keyup click",function(){
                                                                    var prodTotal = $(this).parent().parent().find('.prodTotal');
                                                                    qty = $(this).val();                                                                    
                                                                    total = parseInt(qty) * parseFloat(price,10);
                                                                    if(qty != null && qty != ''){total = parseInt(qty) * parseFloat(price,10);                                                                                                                                              
                                                                        prodTotal.find('.prodTotalNum').text(total.toFixed(2));
                                                                    }
                                                                    allTotal = 0; 
                                                                    cartTable1.find('.prodTotalNum').each(function(){
                                                                        allTotal += parseFloat($(this).text());
                                                                    });
                                                                    cartTable1.find('.rowFooter').find('.subtotalNum').text(allTotal.toFixed(2));                                                                    
                                                                })                                                          
                                                            )
                                                        )
                                                .append($('<td class="prodTotal" title="$'+price+' each"></td>')
                                                            .append('$<span class="prodTotalNum">'+price+'</span>')
                                                        )
                                                .hover(function(){$(this).find('.remove').show();},
                                                        function(){$(this).find('.remove').hide();})                                                
                                          );
                                cartTable1.find('.prodTotalNum').each(function(){
                                    allTotal += parseFloat($(this).text());
                                });                                       
                                cartTable1.find('.rowFooter').find('.subtotalNum').text((allTotal + parseFloat(price)).toFixed(2)); 
                                                                        
                            }                           
                        });                                         
                    
      
                    
                    
                    
                    
                    
                    
                    //LL07042014 Popup product info by cloning hidden items and appending to jqm popup
                    section2.find('.products').find('.prodContainer').find('.prodInfoIcon').unbind('mouseenter mouseleave click')
                        .hover(function(){$(this).css({backgroundColor: '#ECE2D1'});},
                                function(){$(this).css({backgroundColor: ''});})
                        .click(function(){                          
                            $('#prodPop').popup("open");
                            var prodData = $(this).next().clone();
                            prodData.find('select').each(function(){
                                if($(this).children('option').length == 0){
